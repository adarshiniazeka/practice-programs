class Solution(object):
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        n=len(nums)
        total_subsets=(1<<n)
        ans=[]
        for num in range(total_subsets):
            curr_subset=[]
            for i in range(n):
                if(num&(1<<i)!=0):
                    curr_subset.append(nums[i])
            ans.append(curr_subset)
        return(ans)
